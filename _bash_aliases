# Bash HACKs
alias IFS='echo; echo -n "$IFS" | od -abc | grep -A2 0000000; echo'
alias PATH='echo -e ${PATH//:/\\n}'

# Command HACKs
alias sudo='sudo '							# allows sudo to work also on bash aliases


# Quick Path HACKs
alias ~='cd ~'								# User (Home) Directory
alias .='cd .'								# cd .
alias ..='cd ..'							# Up one directory


# Qick Typp HACKs
alias cd.='cd .'							#
alias cd..='cd ..'							#
alias cd-='cd -'							#
alias cd~='cd ~'							#


# Run commands with these default arguments (everytime)
alias cp='cp -v'							# always use Verbose output
alias mv='mv -v'							# always use Verbose outpu
alias rm='rm -vi'							# always use Verbose outputt
alias unxz='unxz --keep'					# keep compressed source file (even after extraction)


# Suppress command errors 
alias clang++='clang++ -x c++'				# 
alias find='find 2>/dev/null'				# send stdout to /dev/null'



# Configuration files
alias bash_profile='vi ~/.bash_profile'		# edit bash_profile configuration
alias hosts='sudo vi /etc/hosts'			# edit hosts file
alias vimrc='vi ~/.vimrc'					# edit vim configuration


